class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int i=0;
        int count=0;
        int n=arr.length;
        for(count =0;count<n;count++){
            if(arr[count]!=0){
                int temp=arr[count];
                arr[count]=arr[i];
                arr[i]=temp;
                i++;
            }
        }
    }
}
